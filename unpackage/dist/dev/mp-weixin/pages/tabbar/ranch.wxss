page {
  background-color: #FCFCFC;
}
.dynamicEvery {
  width: 100%;
  background-color: #FFFFFF;
}
.dynamicEvery .dynamic {
    padding-top: 20rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: start;
    -webkit-justify-content: flex-start;
            justify-content: flex-start;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    position: relative;
}
.dynamicEvery .dynamic .dynamicOne {
      font-size: 36rpx;
      margin-left: 50rpx;
}
.dynamicEvery .dynamic .dynamicClass1 {
      font-weight: bold;
}
.dynamicEvery .dynamicImg {
    width: 86rpx;
    height: 22rpx;
}
.dynamicEvery .dynamicImg1 {
    margin-left: 70rpx;
}
.dynamicEvery .dynamicImg2 {
    margin-left: 250rpx;
}
.dynamicEvery .dynamicImg3 {
    margin-left: 425rpx;
}
.screen {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  height: 80rpx;
  background-color: #FFFFFF;
}
.screen .screenOne {
    line-height: 96rpx;
}
.screen .screenOne .screenOnePicker {
      display: inline-block;
}
.screen .screenOne image {
      width: 30rpx;
      height: 15rpx;
      margin-left: 20rpx;
      line-height: 100%;
}
.box {
  width: 86%;
  margin: 30rpx auto;
  background-color: #FFFFFF;
  padding: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-shadow: 0rpx 0rpx 4rpx 4rpx rgba(79, 82, 139, 0.1);
}
.box .imgBox {
    width: 196rpx;
    height: 140rpx;
}
.box .imgBox .img {
      width: 100%;
      height: 100%;
      background-color: #000000;
}
.box .txtBox {
    width: 60%;
    margin-left: 32rpx;
}
.box .txtBox .txtTitle {
      font-size: 32rpx;
      font-weight: bold;
      line-height: 55rpx;
      color: #333333;
}
.box .txtBox .txtLabel {
      width: 100%;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-flex-wrap: wrap;
              flex-wrap: wrap;
      margin-left: -15rpx;
      margin-bottom: 10rpx;
}
.box .txtBox .txtLabel .txtLabelText {
        font-size: 22rpx;
        border-radius: 20rpx;
        border: 1rpx solid #4E54C8;
        color: #4E54C8;
        padding: 0 18rpx;
        margin: 13rpx 15rpx;
}
.box .txtBox .txtUse {
      float: left;
      font-size: 26rpx;
      color: #999999;
}
.box .txtBox .txtIntegral {
      float: right;
      font-size: 26rpx;
      color: #4E54C8;
}
.printing {
  width: 460rpx;
  height: 160rpx;
  margin: 48rpx auto;
  background-color: #FFFFFF;
  border-radius: 15rpx;
  box-shadow: 0rpx 0rpx 4rpx 4rpx rgba(79, 82, 139, 0.1);
  text-align: center;
  line-height: 160rpx;
}
.printing image {
    width: 62rpx;
    height: 66rpx;
    vertical-align: middle;
}

