<view class="page"><view class="dynamic"><view data-event-opts="{{[['tap',[['tapInformation',[1]]]]]}}" class="{{['dynamicOne',dynamic1===true?'dynamicClass1':'']}}" bindtap="__e">未开始</view><view data-event-opts="{{[['tap',[['tapInformation',[2]]]]]}}" class="{{['dynamicOne',dynamic2===true?'dynamicClass1':'']}}" bindtap="__e">已结束</view><view data-event-opts="{{[['tap',[['tapInformation',[3]]]]]}}" class="{{['dynamicOne',dynamic3===true?'dynamicClass1':'']}}" bindtap="__e">已取消</view></view><view><block wx:for="{{appoint}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><view class="box"><view class="{{['bigCalendar',type===0?'':'bigCalendarBlack']}}"><image src="{{type===0?bigCalendar:bigCalendarBlack}}"></image><text class="bigCalendarDay">{{item.month}}</text><text class="bigCalendarMoney">{{item.day}}</text></view><view class="boxText"><view class="boxLeft"><view class="boxTitle">{{item.theme+"主题"}}</view><view class="boxTime">{{item.begin_time+"-"+item.end_time}}</view><view class="boxPlace">{{item.meet_name}}</view></view><block wx:if="{{type===0}}"><view class="roomPass">{{"门禁密码:"+item.pass}}</view></block></view></view></block></view></view>