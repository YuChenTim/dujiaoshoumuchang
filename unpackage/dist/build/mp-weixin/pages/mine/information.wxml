<view class="page"><view class="headImg"><image src="{{avatarUrl}}"></image><image class="photocamera" src="{{ImgUrl+'static/mine/photocamera.png'}}"></image></view><view class="informationText"><view class="company"><view class="companyTitle">公司</view><view>{{firmName}}</view></view><view class="informationTitle">姓名</view><input class="informationCentent" type="text" placeholder="{{nickname}}" data-event-opts="{{[['input',[['nameChange',['$event']]]]]}}" bindinput="__e"/><view class="informationTitle">职位</view><input class="informationCentent" type="text" placeholder="{{position}}" data-event-opts="{{[['input',[['positionChange',['$event']]]]]}}" bindinput="__e"/><view class="informationTitle">手机号码</view><input class="informationCentent" type="number" placeholder="{{phone}}" data-event-opts="{{[['input',[['phoneChange',['$event']]]]]}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['updateUser',['$event']]]]]}}" class="button" bindtap="__e">保存</button></view>